    if not pdf_files:
        print("‚úó No PDF files found in C:/temp")
        return False
    
    pdf_path = pdf_files[0]
    print(f"Using PDF: {pdf_path.name}")
    print(f"Question (MEDIUM): {QUESTIONS['pdf_medium_revenue']}")
    
    try:
        pdf_data = encode_pdf(pdf_path)
        
        stream = client.chat.completions.create(
            model=MODEL_NAME,
            messages=[
                {
                    "role": "user",
                    "content": [
                        {
                            "type": "file",
                            "file": {
                                "file_data": pdf_data,
                                "filename": pdf_path.name
                            }
                        },
                        {"type": "text", "text": QUESTIONS['pdf_medium_revenue']}
                    ]
                }
            ],
            stream=True,
            max_tokens=200
        )
        
        print("‚úì Streaming response: ", end="", flush=True)
        full_response = ""
